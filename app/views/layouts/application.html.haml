!!! Strict
%html{html_attrs}
  %head
    = render :partial => "shared/head"
    = insert_page_title(@title)
    = yield :head
  %body
    #container
      #branding   
        = render :partial => "shared/banner"
      #navigation 
        = render :partial => "shared/navigation"
      #content.content    
        #notice.flash= flash[:notice]
        = yield :layout
      #footer     
        = render :partial => "shared/footer"
